              class="no-file-selection-message"
              ng-if="rootFolder.children && rootFolder.children.length > 0"
            ><h3>{{ translate "no_selection_select_file" . }}</h3></div>
            <div
              class="no-file-selection-message"
              ng-if="rootFolder.children && rootFolder.children.length === 0"
