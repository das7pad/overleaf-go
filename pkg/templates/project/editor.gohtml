                    <script
                      type="text/ng-template"
                      id="changeEntryTemplate"
                    >
                      <div>
                        <div
                          class="rp-entry-callout"
                          ng-class="'rp-entry-callout-' + entry.type"
                        ></div>
                        <div
                          class="rp-entry-indicator"
                          ng-switch="entry.type"
                          ng-class="{ 'rp-entry-indicator-focused': entry.focused }"
                          ng-click="onIndicatorClick();"
                        ><i
                            class="fa fa-pencil"
                            ng-switch-when="insert"
                          ></i><i
                            class="rp-icon-delete"
                            ng-switch-when="delete"
                          ></i></div>
                        <div
                          class="rp-entry"
                          ng-class="{ 'rp-entry-focused': entry.focused, 'rp-entry-add-comment': entry.type == 'add-comment', 'rp-entry-aggregate-change': entry.type == 'aggregate-change', 'rp-entry-bulk-actions': entry.type == 'bulk-actions', 'rp-entry-comment': entry.type == 'comment', 'rp-entry-delete': entry.type == 'delete', 'rp-entry-insert': entry.type == 'insert' }"
                        >
                          <div class="rp-entry-body">
                            <div
                              class="rp-entry-action-icon"
                              ng-switch="entry.type"
                            ><i
                                class="fa fa-pencil"
                                ng-switch-when="insert"
                              ></i><i
                                class="rp-icon-delete"
                                ng-switch-when="delete"
                              ></i></div>
                            <div class="rp-entry-details">
                              <div
                                class="rp-entry-description"
                                ng-switch="entry.type"
                              >
                                <span ng-switch-when="insert">{{ translate "tracked_change_added" . }}
                                  &nbsp;<ins class="rp-content-highlight">
                                    <span ng-if="isCollapsed">{{`{{ entry.content | limitTo:contentLimit }}`}}</span><span ng-if="!isCollapsed">{{ `{{ entry.content }}` }}</span>
                                  </ins>
                                  <a
                                    class="rp-collapse-toggle"
                                    href="#"
                                    ng-if="needsCollapsing"
                                    ng-click="toggleCollapse();"
                                  ><span ng-if="isCollapsed">…
                                      ({{ translate "show_all" . }}
                                      )</span><span ng-if="!isCollapsed">({{ translate "show_less" . }}
                                      )</span></a></span><span ng-switch-when="delete">{{ translate "tracked_change_deleted" . }}
                                  &nbsp;<del class="rp-content-highlight">
                                    <span ng-if="isCollapsed">{{`{{ entry.content | limitTo:contentLimit}}`}}</span><span ng-if="!isCollapsed">{{ `{{ entry.content }}` }}</span>
                                  </del>
                                  <a
                                    class="rp-collapse-toggle"
                                    href="#"
                                    ng-if="needsCollapsing"
                                    ng-click="toggleCollapse();"
                                  ><span ng-if="isCollapsed">…
                                      ({{ translate "show_all" . }}
                                      )</span><span ng-if="!isCollapsed">({{ translate "show_less" . }}
                                      )</span></a></span></div>
                              <div class="rp-entry-metadata">{{`{{ entry.metadata.ts | date:'MMM d, y h:mm a' }}`}}
                                &nbsp;&bull;&nbsp;<span
                                  class="rp-entry-user"
                                  ng-style="{color: 'hsl({{ `{{ user.hue }}` }}, 70%, 40%)'}"
                                >{{ `{{ user.name }}` }}</span></div>
                            </div>
