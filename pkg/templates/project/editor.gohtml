                                  ng-click="button.handleClick()"
                                  title="{{ `{{ button.title }}` }}"
                                >
                                  <i class="{{ `{{ button.iconClass }}` }}">{{ `{{ button.iconText }}` }}</i>
                                </button>
                              </li>
                            </ul>
                          </div>
